{
	"name": "angular-service",
	"displayName": "Angular Inject Service",
	"publisher": "TheIlPlace",
	"description": "Easily inject angular services",
	"version": "0.1.0",
	"icon": "images/angular-inject.png",
	"galleryBanner": {
		"color": "#0273D4",
		"theme": "dark"
	  },
	"keywords": [
		"Angular",
		"Angular 7",
		"Inject",
		"Services"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/johnpapa/vscode-angular-snippets.git"
	  },
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Snippets"
	],
	"license": "MIT",
	"activationEvents": [
		"onCommand:extension.insertService"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.injectService",
				"title": "Angular Inject Service"
			}
		],
		"keybindings": [
			{
				"command": "extension.injectService",
				"key": "ctrl+shift+i",
				"mac": "cmd+shift+i",
				"when": "editorTextFocus && !editorHasSelection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
